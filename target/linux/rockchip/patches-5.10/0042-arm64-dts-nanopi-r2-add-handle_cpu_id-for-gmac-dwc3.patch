From 8b84ef9618f90ab98f0d3d667c340bb5a5018eb5 Mon Sep 17 00:00:00 2001
From: hmz007 <hmz007@gmail.com>
Date: Sat, 29 Aug 2020 17:17:18 +0800
Subject: [PATCH 42/73] arm64: dts: nanopi-r2: add handle_cpu_id for gmac, dwc3

Signed-off-by: hmz007 <hmz007@gmail.com>
---
 arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi | 2 ++
 1 file changed, 2 insertions(+)

diff --git a/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi b/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi
index d6e51a5b4e59..c23dc592ed3f 100644
--- a/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi
@@ -262,6 +262,7 @@ &gmac2io {
 	snps,txpbl = <0x4>;
 	tx_delay = <0x24>;
 	rx_delay = <0x18>;
+	handle_cpu_id = <1>;
 	status = "okay";
 
 	mdio {
@@ -613,6 +614,7 @@ &usbdrd3 {
 
 &usbdrd_dwc3 {
 	status = "okay";
+	handle_cpu_id = <2>;
 	#address-cells = <1>;
 	#size-cells = <0>;
 
-- 
2.31.1

