From 840884e17a146de5609923573cbeeb75d467d020 Mon Sep 17 00:00:00 2001
From: hmz007 <hmz007@gmail.com>
Date: Thu, 2 Jul 2020 15:50:11 +0800
Subject: [PATCH 39/73] arm64: dts: nanopi-r2: adjust dmc opp-1056 for neo3
 black

Signed-off-by: hmz007 <hmz007@gmail.com>
---
 arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-rev04.dts | 9 +++++++++
 1 file changed, 9 insertions(+)

diff --git a/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-rev04.dts b/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-rev04.dts
index 9fad7e9a3416..c52ce0178b31 100644
--- a/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-rev04.dts
+++ b/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-rev04.dts
@@ -37,6 +37,15 @@ &mach {
 	model = "NanoPi NEO3 Black";
 };
 
+&dmc_opp_table {
+	opp-1056000000 {
+		opp-hz = /bits/ 64 <1056000000>;
+		opp-microvolt = <1200000>;
+		opp-microvolt-L0 = <1200000>;
+		opp-microvolt-L1 = <1175000>;
+	};
+};
+
 &emmc {
 	status = "disabled";
 };
