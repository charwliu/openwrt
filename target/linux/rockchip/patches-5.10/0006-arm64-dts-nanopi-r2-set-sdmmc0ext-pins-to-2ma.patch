From 345c1befb91fcc0e51904164397a2be7de9aea14 Mon Sep 17 00:00:00 2001
From: hmz007 <hmz007@gmail.com>
Date: Tue, 19 Nov 2019 15:13:48 +0800
Subject: [PATCH 06/73] arm64: dts: nanopi-r2: set sdmmc0ext pins to 2ma

Signed-off-by: hmz007 <hmz007@gmail.com>
---
 .../boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi   | 12 ++++++------
 1 file changed, 6 insertions(+), 6 deletions(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi b/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi
index 091919196c03..0d29895f2def 100644
--- a/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi
@@ -356,19 +356,19 @@ wifi_enable_h: wifi-enable-h {
 
 	sdmmc0ext {
 		sdmmc0ext_clk: sdmmc0ext-clk {
-			rockchip,pins = <3 RK_PA2 3 &pcfg_pull_none_4ma>;
+			rockchip,pins = <3 RK_PA2 3 &pcfg_pull_none_2ma>;
 		};
 
 		sdmmc0ext_cmd: sdmmc0ext-cmd {
-			rockchip,pins = <3 RK_PA0 3 &pcfg_pull_up_4ma>;
+			rockchip,pins = <3 RK_PA0 3 &pcfg_pull_up_2ma>;
 		};
 
 		sdmmc0ext_bus4: sdmmc0ext-bus4 {
 			rockchip,pins =
-				<3 RK_PA4 3 &pcfg_pull_up_4ma>,
-				<3 RK_PA5 3 &pcfg_pull_up_4ma>,
-				<3 RK_PA6 3 &pcfg_pull_up_4ma>,
-				<3 RK_PA7 3 &pcfg_pull_up_4ma>;
+				<3 RK_PA4 3 &pcfg_pull_up_2ma>,
+				<3 RK_PA5 3 &pcfg_pull_up_2ma>,
+				<3 RK_PA6 3 &pcfg_pull_up_2ma>,
+				<3 RK_PA7 3 &pcfg_pull_up_2ma>;
 		};
 	};
 
