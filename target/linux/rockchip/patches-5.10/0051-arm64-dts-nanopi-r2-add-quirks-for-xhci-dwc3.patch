From 9996c6a01794624389a8af098f5ec2908f9ec12e Mon Sep 17 00:00:00 2001
From: hmz007 <hmz007@gmail.com>
Date: Thu, 3 Sep 2020 14:09:33 +0800
Subject: [PATCH 51/73] arm64: dts: nanopi-r2: add quirks for xhci, dwc3

Change-Id: I5d797b6123375c7badb641f0abbb793784b80765
Signed-off-by: hmz007 <hmz007@gmail.com>
---
 arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi | 4 ++++
 arch/arm64/boot/dts/rockchip/rk3328.dtsi                  | 2 --
 2 files changed, 4 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi b/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi
index a6fbced0d8f2..71a197bfb57d 100644
--- a/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi
@@ -622,6 +622,10 @@ &usbdrd_dwc3 {
 	#address-cells = <1>;
 	#size-cells = <0>;
 
+	snps,dis-tx-ipgap-linecheck-quirk;
+	snps,xhci-slow-suspend-quirk;
+	snps,xhci-trb-ent-quirk;
+
 	r8153: device@2 {
 		compatible = "usbbda:8153";
 		reg = <2>;
diff --git a/arch/arm64/boot/dts/rockchip/rk3328.dtsi b/arch/arm64/boot/dts/rockchip/rk3328.dtsi
index ce291090ab03..9d1e57ba2a0c 100644
--- a/arch/arm64/boot/dts/rockchip/rk3328.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3328.dtsi
@@ -1013,8 +1013,6 @@ usbdrd_dwc3: dwc3@ff600000 {
 			snps,dis-u3-autosuspend-quirk;
 			snps,dis_u3_susphy_quirk;
 			snps,dis-del-phy-power-chg-quirk;
-			snps,tx-ipgap-linecheck-dis-quirk;
-			snps,xhci-trb-ent-quirk;
 			status = "disabled";
 		};
 	};
-- 
