From 8e66bfc834f2b659a9891c11f253ada6b5e6c813 Mon Sep 17 00:00:00 2001
From: hmz007 <hmz007@gmail.com>
Date: Tue, 19 Nov 2019 19:25:29 +0800
Subject: [PATCH 12/73] arm64: dts: nanopi-r2: set default-sample-phase to 120

Signed-off-by: hmz007 <hmz007@gmail.com>
---
 arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi b/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi
index 0d29895f2def..140c56aba71b 100644
--- a/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3328-nanopi-r2-common.dtsi
@@ -458,7 +458,7 @@ &sdmmc_ext {
 	num-slots = <1>;
 	pinctrl-names = "default";
 	pinctrl-0 = <&sdmmc0ext_clk &sdmmc0ext_cmd &sdmmc0ext_bus4>;
-	rockchip,default-sample-phase = <180>;
+	rockchip,default-sample-phase = <120>;
 	supports-sdio;
 	sd-uhs-sdr104;
 	#address-cells = <1>;
-- 
