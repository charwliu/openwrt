From e93cf376985dc89869db1c10745b56135612de56 Mon Sep 17 00:00:00 2001
From: Frank Wunderlich <frank-w@public-files.de>
Date: Tue, 18 May 2021 19:51:37 +0200
Subject: [PATCH 1040/1041] arm: dts: mt7623: add default_cpu property for
 switch ports

---
 arch/arm/boot/dts/mt7623n-bananapi-bpi-r2.dts | 9 +++++++--
 1 file changed, 7 insertions(+), 2 deletions(-)

diff --git a/arch/arm/boot/dts/mt7623n-bananapi-bpi-r2.dts b/arch/arm/boot/dts/mt7623n-bananapi-bpi-r2.dts
index ce23c9e0ddb2..dda685da2d9c 100644
--- a/arch/arm/boot/dts/mt7623n-bananapi-bpi-r2.dts
+++ b/arch/arm/boot/dts/mt7623n-bananapi-bpi-r2.dts
@@ -228,29 +228,34 @@ ports {
 				port@0 {
 					reg = <0>;
 					label = "wan";
+					default-cpu = <&dsa_cpu1>;
 				};
 
 				port@1 {
 					reg = <1>;
-					label = "lan0";
+					label = "lan1";
+					default-cpu = <&dsa_cpu0>;
 				};
 
 				port@2 {
 					reg = <2>;
-					label = "lan1";
+					label = "lan2";
+					default-cpu = <&dsa_cpu0>;
 				};
 
 				port@3 {
 					reg = <3>;
-					label = "lan2";
+					label = "lan3";
+					default-cpu = <&dsa_cpu0>;
 				};
 
 				port@4 {
 					reg = <4>;
-					label = "lan3";
+					label = "lan4";
+					default-cpu = <&dsa_cpu0>;
 				};
 
-				port@5 {
+				dsa_cpu1: port@5 {
 					reg = <5>;
 					label = "cpu2";
 					ethernet = <&gmac1>;
@@ -263,7 +268,7 @@ fixed-link {
 					};
 				};
 
-				port@6 {
+				dsa_cpu0: port@6 {
 					reg = <6>;
 					label = "cpu";
 					ethernet = <&gmac0>;
-- 
